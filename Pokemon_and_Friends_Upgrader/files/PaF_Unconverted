[insert_hook]
case "PaF: DC Message":{
	PaFDCMessage(LioranBoardJSON.DCWebhookURL,LioranBoardJSON.twitchMessage);
} break
case "PaF: DC Screenshot":{
	PaFDCScreenshot(LioranBoardJSON.DCWebhookURL, LioranBoardJSON.OBSWebsocketURL,LioranBoardJSON.OBSWebsocketPW,LioranBoardJSON.FilePath, LioranBoardJSON.Text);
} break
case "PaF: Create Reward":{
	PaFCreateReward(LioranBoardJSON.Auth_Token,LioranBoardJSON.User_ID,LioranBoardJSON.Name,LioranBoardJSON.Description,LioranBoardJSON.Cost,LioranBoardJSON.Color,LioranBoardJSON.Enabled,LioranBoardJSON.RewardIDVariable);
}break
case "PaF: Enable Reward":{
	PaFEnableReward(LioranBoardJSON.Auth_Token,LioranBoardJSON.User_ID,LioranBoardJSON.RewardIDVariable,LioranBoardJSON.Enabled);
}break
case "PaF: Edit Reward 2":{
	PaFEditReward(LioranBoardJSON.Auth_Token,LioranBoardJSON.User_ID,LioranBoardJSON.Reward_ID,LioranBoardJSON.Skip_Queue,LioranBoardJSON.Input_Text,LioranBoardJSON.Cooldown,LioranBoardJSON.MaxRedeems,LioranBoardJSON.MaxPerUser,LioranBoardJSON.Color);
}break

[insert_script]
function PaFDCMessage(DCWebhookURL,twitchMessage) {
	fetch(DCWebhookURL, {
		method: 'POST',
		headers: {
			'Content-Type': 'application/json'
		},
		body: JSON.stringify({
			'content': twitchMessage
		})
	})
}

async function PaFDCScreenshot(DCWebhookURL,OBSWebsocketURL, OBSWebsocketPW, FilePath, Text) {
  function Base64ToBlob(b64Data, contentType, sliceSize) {

    contentType = contentType || '';
    sliceSize = sliceSize || 512;

    var byteCharacters = atob(b64Data);
    var byteArrays = [];

    for (var offset = 0; offset < byteCharacters.length; offset += sliceSize) {
        var slice = byteCharacters.slice(offset, offset + sliceSize);

        var byteNumbers = new Array(slice.length);
        for (var i = 0; i < slice.length; i++) {
            byteNumbers[i] = slice.charCodeAt(i);
        }

        var byteArray = new Uint8Array(byteNumbers);

        byteArrays.push(byteArray);
    }

    var blob = new Blob(byteArrays, {type: contentType});
    return blob;
  }

  obswsURL = OBSWebsocketURL || '127.0.0.1:4444';
  OBSWebsocketPW = OBSWebsocketPW || '';
  Text = (Text === '0') ? '' : (Text || '');
  
  const obs = new OBSWebSocket();

  obs.connect({
    address: obswsURL,
    password: OBSWebsocketPW
  });
  obs.on('AuthenticationSuccess', async () => {
    try {
      let screenshotData = await obs.send('TakeSourceScreenshot', {
        "sourceName": "PaF - Pokedex Scene",
        "embedPictureFormat": "png",
		"saveToFilePath": FilePath
      });

      let imgFile = (screenshotData.img);
      let block = imgFile.split(";");
      let contentType = block[0].split(":")[1];
      let encodedImage = block[1].split(",")[1];
      let blob = Base64ToBlob(encodedImage, contentType);			
    
      let formData = new FormData();
      formData.append("image", blob, "PaFScreenshot.png");
      formData.append("payload_json", `{"content": "${Text}"}`);

      let requestOptions = {
        method: 'POST',
        body: formData,
        redirect: 'follow'
      };

      let response = await fetch(DCWebhookURL, requestOptions);

      if (response.status == 401) {
        throw "Invalid DCWebhookURL or webhook Token."
      }

    } catch (error) {
      if (typeof error === 'object') {
        error = error.error;
      }

      if (typeof error === 'string') {
      } else {
        console.error(error)
      }
	  LB.alert(error);
    }
    obs.disconnect();      
  })
}
function PaFCreateReward(Auth_Token,User_ID,title,description,cost,color,enabled,RewardIDVariable){
	
	var reward={};
	function LBCreateRewardListener () {
	    var RewardJSON = JSON.parse(this.responseText);
		LB.setVariable(RewardIDVariable, RewardJSON.data[0].id, buttonId = 'global');

	}
	reward.title=title;
	reward.cost=cost
	
	if (description!==""){
		reward.prompt=description;
	}
	if (color!==""){
		reward.background_color=color;
	}
	if (enabled!==""){
		if (enabled==0){
			reward.is_enabled=true;
		}else{
			reward.is_enabled=false;
		}
	}
	//send request to twitch
	reward=JSON.stringify(reward)
	var UpdateReward = new XMLHttpRequest();
	UpdateReward.addEventListener("load", LBCreateRewardListener);
	UpdateReward.open("POST", "https://api.twitch.tv/helix/channel_points/custom_rewards?broadcaster_id="+User_ID);
	UpdateReward.setRequestHeader("Content-Type", "application/json");
	UpdateReward.setRequestHeader("Authorization", "Bearer "+Auth_Token);
	UpdateReward.setRequestHeader("client-id", TWITCH_CLIENT_ID);
	UpdateReward.send(reward);
	return 1;
}
function PaFEditReward(Auth_Token,User_ID,Reward_ID,Skip_Queue,Input_Text,Cooldown,MaxRedeems,MaxPerUser,color){

	var reward={};
	if (Skip_Queue!==""){
		if (Skip_Queue==0){
			reward.should_redemptions_skip_request_queue=true;
		}else{
			reward.should_redemptions_skip_request_queue=false;
		}
	}
	
	if (Input_Text!==""){
		if (Input_Text==0){
			reward.is_user_input_required=true;
		}else{
			reward.is_user_input_required=false;
		}
	}
	
	if (Cooldown > 0) {
		reward.global_cooldown_seconds = Cooldown;
		reward.is_global_cooldown_enabled = true;
	}
	else if (Cooldown !==""){
		reward.global_cooldown_seconds = 0;
		reward.is_global_cooldown_enabled = false;
	}
	
	if (color!==""){
		reward.background_color=color
	}
	
	if (MaxRedeems > 0) {
		reward.max_per_stream  = MaxRedeems;
		reward.is_max_per_stream_enabled = true;
	}
	else if (MaxRedeems !==""){
		reward.max_per_stream = 0;
		reward.is_max_per_stream_enabled = false;
	}
	
	if (MaxPerUser > 0) {
		reward.max_per_user_per_stream = MaxPerUser;
		reward.is_max_per_user_per_stream_enabled = true;
	}
	else if (MaxRedeems !==""){
		reward.max_per_user_per_stream = 0;
		reward.is_max_per_user_per_stream_enabled = false;
	}
	
	//send request to twitch
	reward=JSON.stringify(reward)
	var LBEditRewardCheck = new XMLHttpRequest();
	LBEditRewardCheck.open("PATCH", "https://api.twitch.tv/helix/channel_points/custom_rewards?broadcaster_id="+User_ID+"&id="+Reward_ID);
	LBEditRewardCheck.setRequestHeader("Content-Type", "application/json");
	LBEditRewardCheck.setRequestHeader("Authorization", "Bearer "+Auth_Token);
	LBEditRewardCheck.setRequestHeader("client-id", TWITCH_CLIENT_ID);
	LBEditRewardCheck.send(reward);
	return 1;
}

function PaFEnableReward(Auth_Token,User_ID,Reward_ID,enabled){

var reward={};

if (enabled!==""){
  if (enabled==0){
    reward.is_enabled=true;
  }else{
    reward.is_enabled=false;
  }
}
//send request to twitch
reward=JSON.stringify(reward)
var LBEditRewardCheck = new XMLHttpRequest();
LBEditRewardCheck.open("PATCH", "https://api.twitch.tv/helix/channel_points/custom_rewards?broadcaster_id="+User_ID+"&id="+Reward_ID);
LBEditRewardCheck.setRequestHeader("Content-Type", "application/json");
LBEditRewardCheck.setRequestHeader("Authorization", "Bearer "+Auth_Token);
LBEditRewardCheck.setRequestHeader("client-id", TWITCH_CLIENT_ID);
LBEditRewardCheck.send(reward);
return 1;
}

function PaFLBTimersByK() {
    const timerList = document.getElementById('PaFLBtimersList');
    const timerButton = document.getElementById('PaFLBTimerShow');
    let timers = []
    let init = 0;
    let timerInterval;
    let showInterval;
    let show = false;

    lioranboardclient.on('PaF: Timer Add', (payload) => {
        if (!payload.Data.name || !payload.Data.delay) {
            LB.alert('Timer Extension Error: Name or delay not filled out.');
            return;
        }
        PaFTimerAdd(payload.Data.name, payload.Data.delay, payload.Data.triggerOnce);
    });

    lioranboardclient.on('PaF: Timer Delete', (payload) => {
        if (!payload.Data.name && !payload.Data.deleteAll) {
            LB.alert('Timer Extension Error: Name not filled out.');
            return;
        }
        PaFTimerDelete(payload.Data.name, payload.Data.deleteAll);
    });

    timerButton.onclick = PaFShowTimers;

    function PaFtimerGet (name, variable, global, fromButton) {
    const i = timers.findIndex(t => t.name === name );
    if (i === -1) {
            LB.alert(`Timer Extension Error: Timer named ${name} does not exist. Cannot retrieve current delay.`);
            return;
        }
    const buttonId = global ? 'global' : fromButton
    const value = timers[i].value/1000
    LB.setVariable(variable, value, buttonId)
    }  

    function PaFTimerAdd(name, delay, triggerOnce) {
       const i = timers.findIndex(t => t.name === name );
        if (i !== -1) {
          LB.alert(`Timer Extension Error: Timer named ${name} is already active!`);
            return;
        }
        timers.push ({name, delay: delay * 1000, value: delay * 1000, triggerOnce})
        if (init === 0) {
            init = 1;
            timerInterval = setInterval(() => {
              if (show) {
                  timerList.innerHTML = '';
                  timers.forEach((timer, index) => {
                  const currentDelay = timer.value/1000-1;
                  const formattedDelay = moment.utc(currentDelay * 1000).format('HH:mm:ss');
                  timerList.innerHTML += (` ${timer.name} : ${formattedDelay}  <br/>`);
                  });
                }
                timers.forEach(timer => timer.value = timer.value - 1000)
                const filtered = timers.filter(timer => timer.value <= 0);
                if (filtered.length > 0) {
                filtered.forEach(timer => {
                const i = timers.findIndex(t => t.name === timer.name );
                timers[i].value = timers[i].delay; 
                LB.triggerExt(timer.name);
                if (timer.triggerOnce) {
                timers.splice(i, 1);
              }
                })
                if (timers.length < 1) {
                PaFTimerDelete('all', true);
                }
                }

            }, 1000);
        }
    }

    function PaFTimerDelete(name, deleteAll) {
        if (deleteAll) {
            clearInterval(timerInterval);
            timers = [];
            init = 0;
            timerList.innerHTML = 'No timers are currently active.';
        } else {
            const i = timers.findIndex(t => t.name === name );
            if (i !== -1) {
                timers.splice(i, 1);
            }
            if (timers.length < 1) {
                PaFTimerDelete('all', true);
            }
        }
    }

    function PaFShowTimers() {
        if (timerList.style.display === 'block') {
        timerList.style.display = 'none'; 
		timerList.innerHTML = '';
        timerButton.innerHTML = 'View Active Timers';
        show = false
        return;
        }
        timerButton.innerHTML = 'Hide Active Timers';
        timerList.style.display = 'block';
        show = true

        if (timers.length < 1) {
            clearInterval(showInterval)
            timerList.innerHTML = 'No timers are currently active.';
        }
    }
}
